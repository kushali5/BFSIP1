package Day3;

public class WhileLoop {

	public static void main(String[] args) {
		// Example 1 print 1 to 10
		
	/*	int i=0;       //initiallization
		while(i<=10)  //conditoon
		{
			System.out.println(i);
			i++;     //inc
		}  */
		
		int i=10;
		while(i>=0)
		{
			System.out.println(i);
			i--;
		}
		

	}

}
