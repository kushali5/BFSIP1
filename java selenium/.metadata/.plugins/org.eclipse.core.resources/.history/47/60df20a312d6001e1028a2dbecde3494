package Programs;

public class Bubblesort {

	
	public static void Bubblesort(int arr[])
	{
		for(int i=0; i<arr.length; i++) {
			for(int j=1; j<arr.length-1;j++)
			{
				if (arr[j]<arr[j-1]) {
					//swap
					int temp =arr[j];
					arr[j]=arr[j-1];
					arr[j-1]=temp;
				}
			}
		}
			
	}
	
	
	
	
	
	public static void main(String[] args) {
	int arr[] = {20,13,15,67,89,12};
	

	}

}
