package Programs;

public class PrintArrayElementsinReverse {

	public static void main(String[] args) {
		char a[]= {'r','e','v','e','r','s','e'};
     for(char i=(char)(a.length-1);i>=0;i--) {
		
    	 //int a[]= {1,2,3,4,5};
		//for(int i=a.length-1;i>=0;i--)
		
		
		System.out.println(a[i]);
		}
	}

}
